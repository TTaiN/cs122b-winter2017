    <servlet-class>Main</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Browse</servlet-name>
    <servlet-class>Browse</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Search</servlet-name>
    <servlet-class>Search</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>MovieList</servlet-name>
